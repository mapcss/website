{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "trailingSlash": true,
    "rewrites": [{
      "source": "/playground",
      "destination": "/index.html"
    }]
  },
  "emulators": {
    "hosting": {
      "port": 5000
    }
  }
}
